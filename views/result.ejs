<%- include ("./partials/header") %>
<h1 class="pageNumber">
  Here is a list of all of the pictures on page: <%= pageNumber %>
</h1>
<div class="result">
  <% for(var i = 0;i < picData.results.length;i++){ %>
  <div <%="picData.results[i].color" %>
    ;>
    <img
      class="resp"
      src="<%= picData.results[i].urls.thumb %>"
      
    />
    
    <p>
      Dimensions: <%= picData.results[i].width %>X<%= picData.results[i].height
      %>
    </p>
   
  </div>
  <% } %>
</div>
<%- include ("./partials/footer") %>
